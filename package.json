{
  "name": "neutron",
  "productName": "neutron",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Reginaldo Junior",
    "email": "reginaldo.junior696@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "asar": true
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "neutron"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/background/index.html",
                  "js": "./src/renderer/background/background.tsx",
                  "name": "background_window"
                },
                {
                  "html": "./src/renderer/main/index.html",
                  "js": "./src/renderer/main/main.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ],
        [
          "@electron-forge/plugin-auto-unpack-natives"
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.50",
    "@electron-forge/plugin-webpack": "6.0.0-beta.50",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/memoizee": "^0.4.3",
    "@types/mv": "^2.1.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/styled-components": "^5.1.0",
    "@types/tar-fs": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.0.0",
    "electron": "11.5.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "fs-extra": "^9.0.0",
    "node-loader": "^0.6.0",
    "node.extend": "^2.0.2",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.0"
  },
  "dependencies": {
    "@types/auto-launch": "^5.0.1",
    "@types/lodash": "^4.14.150",
    "@types/react-virtualized": "^9.21.9",
    "auto-launch": "^5.0.5",
    "chokidar": "^3.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "fix-path": "^3.0.0",
    "lodash": "^4.17.15",
    "memoizee": "^0.4.14",
    "mv": "^2.1.1",
    "node.flow": "^1.2.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-virtualized": "^9.21.2",
    "rmdir": "^1.2.0",
    "styled-components": "^5.1.0",
    "tar-fs": "^2.0.1"
  }
}
